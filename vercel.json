{
  "builds": [
    {
      "src": "_website/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static_files"
      }
    }
  ],
  "routes": [
    {
      "src": "/static_files/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "_website/wsgi.py"
    }
  ]
}