{
  "builds": [
    {
      "src": "_core/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "static_files" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "_core/wsgi.py"
    }
  ]
}